{
  "endpoints": {
    "sports_players": {
      "url": "https://statsapi.mlb.com/api/v1/sports/{sportId}/players",
      "description": "Get a list of all players for a specific sport and season. Returns detailed player information including IDs, names, birth details, and handedness. This endpoint supports fields parameter to limit response data.",
      "required_params": ["sportId", "season"],
      "optional_params": ["gameType", "fields"],
      "rate_limit": "25 requests per second",
      "case_sensitive": true,
      "example_calls": [
        {
          "description": "Get MLB players for 2024 season with specific fields",
          "url": "https://statsapi.mlb.com/api/v1/sports/1/players?season=2024&fields=people,id,fullName",
          "notes": "sportId=1 represents MLB"
        }
      ],
      "response_schema": {
        "people": [{
          "id": "Integer - Unique identifier for the player",
          "fullName": "String - Player's full name",
          "birthDate": "String - YYYY-MM-DD format",
          "birthCity": "String",
          "birthStateProvince": "String",
          "batSide": "String - Left/Right",
          "pitchHand": "String - Left/Right"
        }]
      },
      "pagination": {
        "supported": false
      }
    },
    "schedule": {
      "url": "https://statsapi.mlb.com/api/v1/schedule",
      "description": "Get schedule information with extensive filtering options. Supports detailed game data through hydration parameters.",
      "required_params": ["sportId", "gamePk", "gamePks"],
      "optional_params": [
        "scheduleType",
        "eventTypes",
        "hydrate",
        "teamId",
        "leagueId", 
        "venueIds",
        "gameTypes",
        "date",
        "startDate",
        "endDate",
        "season",
        "opponentId",
        "fields"
      ],
      "rate_limit": "25 requests per second",
      "case_sensitive": true,
      "pagination": {
        "supported": true,
        "params": {
          "limit": "Number of records to return",
          "offset": "Starting point for records (i+1)"
        }
      },
      "example_calls": [
        {
          "description": "Get MLB schedule for a specific date range",
          "url": "https://statsapi.mlb.com/api/v1/schedule?sportId=1&startDate=2024-04-01&endDate=2024-04-07&fields=dates,games,teams,venue"
        }
      ]
    },
    "game": {
      "url": "https://statsapi.mlb.com/api/v1/game/{gamePk}/feed/live",
      "description": "Get detailed live game feed including play-by-play data, player stats, and game state information. Supports timecode parameter for historical snapshots.",
      "required_params": ["gamePk"],
      "optional_params": ["timecode", "hydrate", "fields"],
      "rate_limit": "25 requests per second",
      "case_sensitive": true,
      "example_calls": [
        {
          "description": "Get live game feed at specific moment",
          "url": "https://statsapi.mlb.com/api/v1/game/531060/feed/live?timecode=20240401_182458",
          "notes": "Timecode format: YYYYMMDD_HHMMSS"
        }
      ]
    },
    "team_stats": {
      "url": "https://statsapi.mlb.com/api/v1/teams/{teamId}/stats",
      "description": "Get comprehensive team statistics with support for various stat types, groups, and situational splits. Allows filtering by season and game type.",
      "required_params": ["teamId"],
      "optional_params": [
        "season",
        "statGroup",
        "gameType",
        "stats",
        "sportIds",
        "sitCodes",
        "fields"
      ],
      "notes": "Use meta('statGroups') for valid group values, meta('statTypes') for valid stats values, and meta('situationCodes') for valid sitCodes. Use sitCodes with stats=statSplits.",
      "rate_limit": "25 requests per second",
      "case_sensitive": true,
      "example_calls": [
        {
          "description": "Get team batting stats for specific situations",
          "url": "https://statsapi.mlb.com/api/v1/teams/147/stats?stats=statSplits&statGroup=hitting&sitCodes=risp,men_on&season=2024",
          "notes": "Team 147 represents the New York Yankees"
        }
      ]
    },
    "people": {
      "url": "https://statsapi.mlb.com/api/v1/people",
      "description": "Get detailed information about multiple players. Supports hydration for additional related data and fields parameter for response filtering.",
      "required_params": ["personIds"],
      "optional_params": ["hydrate", "fields"],
      "rate_limit": "25 requests per second",
      "case_sensitive": true,
      "example_calls": [
        {
          "description": "Get specific information for multiple players",
          "url": "https://statsapi.mlb.com/api/v1/people?personIds=671096,660271&fields=people,id,fullName,primaryPosition",
          "notes": "Comma-separated list of player IDs"
        }
      ]
    },
    "stats": {
      "url": "https://statsapi.mlb.com/api/v1/stats",
      "description": "Get detailed statistics with extensive filtering and grouping options. Results limited to 50 records by default if no limit specified.",
      "required_params": ["stats", "group"],
      "optional_params": [
        "playerPool",
        "position",
        "teamId",
        "leagueId",
        "limit",
        "offset",
        "gameType",
        "season",
        "sportIds",
        "sortStat",
        "order",
        "hydrate",
        "fields",
        "personId",
        "metrics",
        "startDate",
        "endDate"
      ],
      "rate_limit": "25 requests per second",
      "case_sensitive": true,
      "pagination": {
        "supported": true,
        "default_limit": 50,
        "params": {
          "limit": "Number of records to return",
          "offset": "Starting point for records (i+1)"
        }
      },
      "example_calls": [
        {
          "description": "Get batting stats for specific team",
          "url": "https://statsapi.mlb.com/api/v1/stats?stats=season&group=hitting&teamId=147&season=2024&fields=stats,splits,stat",
          "notes": "Group parameter supports: hitting, pitching, fielding, running, game, team"
        }
      ]
    }
  },
